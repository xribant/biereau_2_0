<form method="post" action="{{ path('admin.agenda.delete', {'id': agenda.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ agenda.id) }}">
    <button class="btn">Supprimer l'évènement</button>
</form>
